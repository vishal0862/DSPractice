/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		for(int i=0; i<a; i++)
		{
		    char[]  s1=sc.next().toCharArray();//this method converts string into single characters 
		    char[] s2=sc.next().toCharArray();
		    for(int j=0; j<5; j++)
		    {
		        if(s1[j]==s2[j])
		        {
		            System.out.print("G");
		      
		        }
		        else
		            System.out.print("B");
		    }
		    System.out.println();
		}
	}
}
