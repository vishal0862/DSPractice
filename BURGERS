



import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		while(a-->0)
		{
		    int b=sc.nextInt();
		    int c=sc.nextInt();
		    if(b<c)
		        System.out.println(b);
		    else
		        System.out.println(c);
		}
		// your code goes here
	}
}
